#include <vector>
#include <math.h>


double f(double x); //Declaration

int main() {

    // Create a vector<double> of length 11 with x-es
    std::vector<double> v(11);

    // Assign numbers to the vector
    double n = 0;
    for (size_t i = 0; i < v.size(); i++)
     {
     v[i] = n;
     n += 0.1;
     }

    // Create a vector<double> of length 11 with u(x)-es
    std::vector<double> u(11);

    for (size_t i = 0; i < v.size(); i++)
     {
     u[i] = f(v[i]);
     }
    return 0;
}

double f(double x){
    //Provide definition of function here:
    return 1 - (1-exp(-10))*x - exp(-10*x);
}